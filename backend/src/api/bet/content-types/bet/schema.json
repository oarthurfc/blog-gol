{
  "kind": "collectionType",
  "collectionName": "bets",
  "info": {
    "singularName": "bet",
    "pluralName": "bets",
    "displayName": "Bet"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "stars": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "differential": {
      "type": "string",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::article.article"
    },
    "checklist_1": {
      "type": "string"
    },
    "checklist_2": {
      "type": "string"
    },
    "checklist_3": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "ranking": {
      "type": "integer",
      "required": true,
      "unique": true
    }
  }
}
